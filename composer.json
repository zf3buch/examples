{
  "name": "zf3buch/examples",
  "description": "Some examples to play around with Zend Framework 3",
  "license": "The MIT License (MIT)",
  "type": "project",
  "authors": [
    {
      "name": "Ralf Eggert",
      "email": "ralf@travello.de",
      "homepage": "http://www.travello.de",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/zf3buch/examples",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "zendframework/zend-authentication": "^2.5",
    "zendframework/zend-cache": "^2.7",
    "zendframework/zend-code": "^3.0",
    "zendframework/zend-crypt": "^3.0",
    "zendframework/zend-config": "^2.6",
    "zendframework/zend-console": "^2.6",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-debug": "^2.5",
    "zendframework/zend-diactoros": "^1.3",
    "zendframework/zend-escaper": "^2.5",
    "zendframework/zend-eventmanager": "^3.0",
    "zendframework/zend-filter": "^2.7",
    "zendframework/zend-form": "^2.9",
    "zendframework/zend-http": "^2.5",
    "zendframework/zend-hydrator": "^2.2",
    "zendframework/zend-i18n": "^2.7",
    "zendframework/zend-json": "^3.0",
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-log": "^2.9",
    "zendframework/zend-math": "^3.0",
    "zendframework/zend-modulemanager": "^2.7",
    "zendframework/zend-mvc-console": "^1.1",
    "zendframework/zend-navigation": "^2.8",
    "zendframework/zend-paginator": "^2.7",
    "zendframework/zend-permissions-acl": "^2.6",
    "zendframework/zend-permissions-rbac": "^2.5",
    "zendframework/zend-serializer": "^2.8",
    "zendframework/zend-servicemanager": "^3.1",
    "zendframework/zend-session": "^2.7",
    "zendframework/zend-stdlib": "^3.0",
    "zendframework/zend-stratigility": "^1.2",
    "zendframework/zend-test": "^3.0",
    "zendframework/zend-validator": "^2.8",
    "zendframework/zend-view": "^2.8",
    "twitter/bootstrap": "~3.3",
    "fortawesome/font-awesome": "~4.4",
    "frameworks/jquery": "~1.11"
  },
  "autoload": {
    "psr-4": {
      "Basket\\": "src/Basket",
      "Book\\": "src/Book",
      "Customer\\": "src/Customer",
      "DesignPatternExamples\\": "src/DesignPatternExamples",
      "Pizza\\": "src/Pizza",
      "Scripts\\": "script/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BasketTest\\": "tests/Basket/"
    }
  },
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
    "post-update-cmd": "Scripts\\Composer::postUpdate"
  }
}
